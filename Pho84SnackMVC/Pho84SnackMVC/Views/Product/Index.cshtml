@model IEnumerable<Pho84SnackMVC.Models.Product>

@{
   ViewData["Title"] = "Index";
   var productTitleActions = new TitleWithRightActionsViewModel("Products", Url.Action("create", "product"));
   var linkslistModel = Model.Select(m => new LinksEntryViewModel("details", m.Id, m.Name));
}
<partial name="_TitleWithRightActionsPartial" model="productTitleActions" />
<partial name="_LinksListPartial" model="linkslistModel" />