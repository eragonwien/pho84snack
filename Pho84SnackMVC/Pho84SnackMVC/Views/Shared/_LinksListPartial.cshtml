@model IEnumerable<LinksEntryViewModel>
@{
   string currentUrl = Context.Request.Path;
}
<div class="mt-16">
   @if (Model.Count() == 0)
   {
      <p class="has-text-centered">No item found</p>
   }
   else
   {
      <div class="panel">
         @foreach (var entry in Model)
         {
            <div class="panel-block">
               <a asp-action="@entry.Action" asp-route-id="@entry.Id" class="overflow-x">
                  <span>@entry.DisplayName</span>
               </a>
            </div>
         }
      </div>
   }
</div>